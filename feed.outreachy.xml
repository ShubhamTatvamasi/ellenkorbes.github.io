---

<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
 <channel>
  <title>{{ site.title  | xml_escape }} - outreachy</title>
  <description>Posts tagged as 'outreachy'</description>
     <atom:link href="{{ site.url }}/feed.blog.xml" rel="self" type="application/rss+xml" />
  <link>{{ site.url }}/tag/blog/</link>

  {% for post in site.tags.outreachy limit:10 %}
   <item>
    <title>{{ post.title | xml_escape }}</title>
    <description>{{ post.content | xml_escape }}</description>
    <link>{{ site.url }}{{ post.url }} </link>
    <pubDate>{{ post.date | date_to_rfc822 }}</pubDate>
    <guid isPermaLink="true">{{ site.url }}{{ post.url }}</guid>
   </item>
  {% endfor %}
 </channel>
</rss>